{
    "name": "mukaosolu/basic-php-composer",
    "description": "A simple PHP app with Carbon and PHPUnit, CI-ready",
    "require": {
      "php": "^8.2",  
      "nesbot/carbon": "^3.9"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.6"
    },
    "scripts": {
      "test": "phpunit --configuration phpunit.xml"
    },
    "minimum-stability": "stable",
    "license": "MIT"
  }
  